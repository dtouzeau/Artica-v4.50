function UserAgentToIcon($UA):string{
    if (preg_match("# OPR\/[0-9\.]+#", $UA)) {return "fab fa-opera";}
    if (preg_match("#Edge\/#", $UA)) { return "fab fa-edge"; }
    if (preg_match("#Firefox\/#", $UA)) {return "fab fa-firefox";}
    if (preg_match("#Chrome/[0-9\.]+#", $UA)) {return "fab fa-chrome";}
    if (preg_match("#Safari/[0-9\.]+#", $UA)) {return "fab fa-chrome";}
    if (preg_match("#Trident\/[0-9]+\.#", $UA)) {return "fab fa-internet-explorer";}
    if (preg_match("#MSIE\s+[0-9\.]+#", $UA)) {return "fab fa-internet-explorer";}
    return "fas fa-browser";
}