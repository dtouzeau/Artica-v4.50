	h1{font:bold 24px;margin:0}
	h2{font:bold 20px;margin:0;}
	h4{font:bold 12px arial; margin:0}
	p{text-align:justify;line-height:1.3;margin:0 0 0.5em 0}
	.z{border:1px solid red}
	.h1px{height:1px;font-size:1px;line-height:1px}
	ul{margin:6px 0 6px 20px;padding:0}
	ul li{margin:3px 0}
	#titles h1,h2 {color:black;font-family:'Tahoma', 'Verdana';color:black;text-align:center}
	#content,#footer{	font-family:'Tahoma', 'Verdana';color:black;}
	blockquote {margin:5px;padding:0px;font-size:18px;text-align:center;width:100%}
	#error{text-align:center;font-size:20px;}
	#error p{text-align:center;font-size:22px;}
	hr{width:80%;text-aling:center;border:1px solid black;}
	#footer{text-align:center;padding:0 2px 2px;margin-left:-30px;margin-right:-30px;margin-bottom:-40px}
	#footer p{padding:12px 30px 14px;text-align:center;font-size:16px;color:#949494;}
	pre{font-size:20px;color:black;font-family:'Tahoma', 'Verdana';font-weight:bold}
</style>
</head>
